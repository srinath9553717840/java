package com.nt.beans;


import java.io.IOException;
import java.util.ArrayList;

public class Emp {
	
public static void main(String[] args) throws IOException{
	

	int n=5;
	
	ArrayList<Integer> ar=new  ArrayList<Integer>(n);
	
	for (int i = 0; i <=n ; i++) {
		
		ar.add(i);
		System.out.println(ar);
		ar.remove(2);
		System.out.println(ar);
		
	}
	
	}
		
	}
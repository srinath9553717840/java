package com.nt.beans;


import java.io.IOException;
import java.util.ArrayList;

public class Emp {
	
public static void main(String[] args){
	
	int n=10;
	
ArrayList<Integer> al=new ArrayList<Integer>();
   for (int i = 0; i <=n ; i++) {
	   
	   al.add(i);
	   System.out.println(al);
	
	   al.remove(2);
	   System.out.println(al);
}

	
	}
}
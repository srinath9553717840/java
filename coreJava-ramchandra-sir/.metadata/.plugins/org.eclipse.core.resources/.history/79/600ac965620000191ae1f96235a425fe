package com.nt.beans;


import java.io.IOException;
import java.util.ArrayList;

public class Emp {
	
public static void main(String[] args) {
	
	int n=12;
	ArrayList<Integer> ar=new ArrayList<Integer>(n);
	for (int i = 1; i <=10; i++) {
		ar.add(i);
			System.out.println(ar);
			System.out.println();
	ar.remove(3);
	System.out.println();
		
	}

	
}
}